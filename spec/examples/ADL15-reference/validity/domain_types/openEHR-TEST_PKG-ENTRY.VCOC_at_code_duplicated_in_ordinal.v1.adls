archetype (adl_version=1.5)
	openEHR-TEST_PKG-ENTRY.VCOC_at_code_duplicated_in_ordinal.v1

language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Peter Gummer">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"Test VCOC validity check, triggered when an at-code is duplicated in an ordinal.">
			keywords = <"ADL", "validation", "test">
			copyright = <"copyright (c) 2008 The openEHR Foundation">
		>
	>
	other_details = <
		["regression"] = <"VCOC">
	>
	lifecycle_state = <"draft">

definition
	ENTRY[at0000] matches {
		value matches {
			DV_ORDINAL matches {
				[value, symbol] matches {
					[{0}, {[local::at0001]}], 	-- +
					[{1}, {[local::at0002]}], 	-- ++
					[{2}, {[local::at0002]}], 	-- ++
					[{3}, {[local::at0003]}] 	-- +++
				}
			}
		}
	}

